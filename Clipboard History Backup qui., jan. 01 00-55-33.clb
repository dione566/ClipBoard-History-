#!/bin/bash

# ==================================
# --- CONFIGURA√á√ÉO DE BOTS ---
# ==================================
BOTS=(
    "/sdcard/Download/Bot/botweb1"
    "/sdcard/Download/Bot/botweb2"
)
SCRIPT_PYTHON="main.py"
LOG_PREFIX="execucao" # O arquivo de log ser√°: <DIRETORIO>/<LOG_PREFIX>.log

# Vari√°vel para armazenar os PIDs dos processos Python
declare -A PIDS # Array associativo para PIDs: Chave = Diret√≥rio, Valor = PID

# ==================================
# --- FUN√á√ïES ---
# ==================================

# Fun√ß√£o para registrar eventos
log_message() {
    local log_file="$1"
    local message="$2"
    echo "$(date '+%Y-%m-%d %H:%M:%S') - $message" >> "$log_file"
}

# Fun√ß√£o de limpeza para lidar com Ctrl+C
cleanup() {
    echo -e "\nCtrl+C detectado. Tentando fechar os bots..."
    
    local all_clean=true
    
    # Itera sobre os PIDs registrados
    for diretorio in "${!PIDS[@]}"; do
        local pid="${PIDS[$diretorio]}"
        local log_file="$diretorio/$LOG_PREFIX.log"
        
        # Garante que o arquivo de log exista para registrar a interrup√ß√£o
        touch "$log_file"
        log_message "$log_file" "SINAL DE INTERRUP√á√ÉO (Ctrl+C) RECEBIDO para $diretorio. Tentando fechar o bot (PID: $pid)..."

        # Verifica se o PID est√° definido e ativo
        if [ ! -z "$pid" ] && kill -0 "$pid" 2>/dev/null; then
            echo "-> Enviando sinal de TERMina√ß√£o ($pid) para $diretorio..."
            kill -SIGTERM "$pid"
            
            # Espera um pouco, at√© 5 segundos, pelo encerramento limpo
            local timeout=5
            local count=0
            while kill -0 "$pid" 2>/dev/null && [ $count -lt $timeout ]; do
                sleep 1
                count=$((count+1))
            done
            
            if kill -0 "$pid" 2>/dev/null; then
                # Se ainda estiver rodando ap√≥s o timeout, envia SIGKILL (Encerramento For√ßado)
                echo "-> O bot $diretorio ($pid) n√£o respondeu. Enviando SIGKILL!"
                kill -SIGKILL "$pid"
                wait "$pid" 2>/dev/null
                log_message "$log_file" "FECHAMENTO INTERROMPIDO FOR√áADO (SIGKILL): O script foi encerrado √† for√ßa."
                echo "Aviso: O bot $diretorio foi encerrado √† for√ßa (SIGKILL)."
                all_clean=false
            else
                wait "$pid" 2>/dev/null # Espera a termina√ß√£o limpa
                CODIGO_SAIDA=$?
                log_message "$log_file" "FECHAMENTO INTERROMPIDO LIMPO: O script foi encerrado com sucesso (C√≥digo de sa√≠da: $CODIGO_SAIDA)."
                echo "-> Fechamento Limpo: O bot $diretorio foi encerrado corretamente."
            fi
        else
            log_message "$log_file" "FECHAMENTO INTERROMPIDO: O bot $diretorio n√£o estava em execu√ß√£o ou o PID era desconhecido."
            echo "-> O bot $diretorio n√£o estava em execu√ß√£o ou n√£o foi necess√°rio encerrar um processo ativo."
        fi
    done

    if $all_clean; then
        echo -e "\nTODOS OS BOTS FORAM ENCERRADOS LIMPADAMENTE."
    else
        echo -e "\nAVISO: PELO MENOS UM BOT FOI ENCERRADO √Ä FOR√áA."
    fi
    
    echo "FIM DA EXECU√á√ÉO (INTERROMPIDA)"
    exit 130 # C√≥digo de sa√≠da padr√£o para interrup√ß√£o por Ctrl+C
}

# Configura o trap para o sinal INT (Ctrl+C)
trap cleanup INT

# ==================================
# --- SCRIPT PRINCIPAL ---
# ==================================

# 0. Limpar a tela (janela de comando)
clear
echo "--- INICIANDO ROTINA DE BOTS PARALELOS ---"

# 1. Limpar e Registrar In√≠cio (Limpa o log a cada execu√ß√£o)
for diretorio in "${BOTS[@]}"; do
    log_file="$diretorio/$LOG_PREFIX.log"
    
    # Verifica se o diret√≥rio existe antes de tentar limpar o log
    if [ -d "$diretorio" ]; then
        echo "Limpando o arquivo de log: $log_file"
        > "$log_file" # Limpa o log (cria se n√£o existir)
        log_message "$log_file" "IN√çCIO DA EXECU√á√ÉO DO BOT"
        echo "-> Verifique $log_file para detalhes do bot em $diretorio."
    else
        echo "ERRO: O diret√≥rio $diretorio n√£o existe. N√£o √© poss√≠vel limpar o log."
    fi
done

echo -e "\n--- INICIANDO EXECU√á√ÉO PARALELA ---"

# 2. Execu√ß√£o dos Bots em Paralelo
for diretorio in "${BOTS[@]}"; do
    log_file="$diretorio/$LOG_PREFIX.log"
    
    # Navega√ß√£o para o diret√≥rio
    if ! cd "$diretorio"; then
        log_message "$log_file" "ERRO FATAL: N√£o foi poss√≠vel navegar para o diret√≥rio $diretorio."
        echo "ERRO: O diret√≥rio $diretorio n√£o existe ou as permiss√µes est√£o incorretas. Pulando este bot."
        continue # Pula para o pr√≥ximo bot
    fi

    # Execu√ß√£o do script Python em segundo plano e captura do PID
    echo "Executando $SCRIPT_PYTHON em $diretorio em segundo plano..."
    log_message "$log_file" "Executando comando: python $SCRIPT_PYTHON em segundo plano"
    
    python "$SCRIPT_PYTHON" &
    PIDS[$diretorio]=$! # Captura o Process ID (PID) do √∫ltimo comando em background
    
    echo "Bot $diretorio PID: ${PIDS[$diretorio]}"
    
    # Retorna para o diret√≥rio de onde o script foi chamado (boa pr√°tica)
    cd - > /dev/null
done

if [ ${#PIDS[@]} -eq 0 ]; then
    echo -e "\nNenhum bot p√¥de ser iniciado. Saindo."
    trap - INT
    exit 1
fi

echo -e "\nTODOS OS BOTS EST√ÉO EM EXECU√á√ÉO. Pressione Ctrl+C para fechar todos."

# 3. Espera pelos Bots
# Cria uma lista de PIDs para esperar
pids_to_wait=()
for pid in "${PIDS[@]}"; do
    pids_to_wait+=("$pid")
done

# Espera que TODOS os scripts Python terminem.
# O script ficar√° preso aqui at√© que todos os bots morram naturalmente ou seja interrompido por Ctrl+C.
wait "${pids_to_wait[@]}"

# 4. Verifica√ß√£o de Fechamento (Sa√≠da normal)
trap - INT # Remove o trap para que a limpeza n√£o seja chamada duas vezes ao sair

echo -e "\n--- VERIFICA√á√ÉO DE FECHAMENTO NORMAL ---"

# Itera novamente sobre os diret√≥rios para registrar o fim da execu√ß√£o
for diretorio in "${BOTS[@]}"; do
    # Verifica se o diret√≥rio estava na lista de PIDs iniciados com sucesso
    if [ ! -z "${PIDS[$diretorio]}" ] && [ -f "$diretorio/$LOG_PREFIX.log" ]; then
        log_message "$diretorio/$LOG_PREFIX.log" "FECHAMENTO NORMAL: O script Python terminou."
        echo "-> O bot $diretorio terminou sua execu√ß√£o."
    fi
done

echo -e "\nEXECU√á√ÉO CONCLU√çDA."

exit 0 # Sai com c√≥digo de sucesso
